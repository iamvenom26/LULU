    <style> 
        /* Add these basic styles at the top */
        body {
            margin: 0;
            height: 100vh;
            overflow: hidden;
            background: #000;
            font-family: 'Arial', sans-serif;
        }

        #background {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: linear-gradient(to bottom, #000428, #004e92);
            opacity: 0;
            animation: fadeIn 2s ease-in forwards;
        }

        #logo-container {
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            display: flex;
            flex-direction: column;
            align-items: center;
        }

        @keyframes fadeIn {
            from { opacity: 0; }
            to { opacity: 1; }
        }

        /* Then keep all your existing styles */
        .cube-container {
            perspective: 1000px;
            width: 200px;
            height: 200px;
            margin: 50px auto;
            opacity: 0;
            animation: showCube 1s ease forwards;
            animation-delay: 1s;
        }

        .cube {
            width: 100%;
            height: 100%;
            position: relative;
            transform-style: preserve-3d;
            animation: rotateCube 20s infinite linear;
        }

        .face {
            position: absolute;
            width: 200px;
            height: 200px;
            background: rgba(0, 212, 255, 0.1);
            border: 2px solid #00d4ff;
            backface-visibility: visible;
        }

        .grid-face {
            width: 100%;
            height: 100%;
            display: grid;
            grid-template-columns: repeat(4, 1fr);
            grid-template-rows: repeat(4, 1fr);
            gap: 2px;
            padding: 4px;
        }

        .grid-item {
            display: flex;
            align-items: center;
            justify-content: center;
            color: #ffffff;
            background: rgba(0, 212, 255, 0.05);
            border: 1px solid rgba(0, 212, 255, 0.3);
        }

        .grid-item:nth-child(odd) {
            font-size: 1.8rem;
            font-weight: bold;
        }

        .grid-item:nth-child(even) {
            font-size: 1.5rem;
        }

        .front { transform: translateZ(100px); }
        .back { transform: rotateY(180deg) translateZ(100px); }
        .right { transform: rotateY(90deg) translateZ(100px); }
        .left { transform: rotateY(-90deg) translateZ(100px); }
        .top { transform: rotateX(90deg) translateZ(100px); }
        .bottom { transform: rotateX(-90deg) translateZ(100px); }

        @keyframes rotateCube {
            0% { transform: rotateX(0deg) rotateY(0deg); }
            100% { transform: rotateX(360deg) rotateY(360deg); }
        }

        .braces-container {
            display: flex;
            align-items: center;
            justify-content: center;
            gap: 60px;
            margin: 50px auto;
        }

        .brace {
            font-size: 10rem;
            color: #00d4ff;
            opacity: 0;
            transform: scale(0.5);
        }

        .brace.left {
            animation: openLeft 1s ease forwards;
        }

        .brace.right {
            animation: openRight 1s ease forwards;
        }

        @keyframes openLeft {
            0% {
                opacity: 0;
                transform: translateX(50px) scale(0.5);
            }
            100% {
                opacity: 1;
                transform: translateX(0) scale(1);
            }
        }

        @keyframes openRight {
            0% {
                opacity: 0;
                transform: translateX(-50px) scale(0.5);
            }
            100% {
                opacity: 1;
                transform: translateX(0) scale(1);
            }
        }

        @keyframes showCube {
            0% {
                opacity: 0;
                transform: scale(0.5);
            }
            100% {
                opacity: 1;
                transform: scale(1);
            }
        }

        .presents-text {
            font-size: 1.8rem;
            color: #00d4ff;
            margin-top: 40px;
            opacity: 0;
            animation: fadeIn 1s ease forwards;
            animation-delay: 2.5s;
            text-transform: uppercase;
            letter-spacing: 2px;
            font-weight: 500;
        }

        .presents {
            font-size: 3.5rem;
            font-weight: bold;
            color: #ffffff;
            opacity: 0;
            letter-spacing: 0.2em;
            animation: fadeIn 1s ease forwards;
            animation-delay: 3s;
            text-shadow: 0 0 10px rgba(255, 255, 255, 0.3);
            margin-top: 10px;
        }
         
    </style>
</head>
